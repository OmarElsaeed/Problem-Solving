#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s;
    cin>>s;
    int counter = s.length();
    sort(s.begin(),s.end());
    for(int i =0;i<s.length();i++)
    {
        if(s[i]==s[i+1]){counter--;}
    }
    if(counter%2==0)cout<<"CHAT WITH HER!\n";
    else cout<<"IGNORE HIM!\n";
    return 0;
}
