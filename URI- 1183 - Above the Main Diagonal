#include <bits/stdc++.h>

using namespace std;

int main()
{
   char c;
   cin>>c;
   int cnt = 0;
   double arr[12][12],sum = 0.0;
   for(int i = 0; i < 12; i++){
    for(int j = 0; j <12; j++){
    cin>>arr[i][j];
    if(j > i) {sum += arr[i][j]; cnt++;}
   }}
   if(c == 'S') cout<<sum;
   else {
    cout<< sum / cnt<<endl;
   }
    return 0;
}
