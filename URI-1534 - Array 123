#include <iostream>
#include <stdio.h>

using namespace std;

int main(){
    int n;
    while(cin>>n){

        for(int i = 0;i<n;i++){
            for(int j = 0;j<n;j++){
                if(n-i == j+1){cout<<2;continue;}
                if(j == i) {cout<<1;continue;}
                cout<<3;
                }
                cout<<endl;
            }
        }
    return 0;
}
