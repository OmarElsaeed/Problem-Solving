#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s,t;
    while(cin>>s>>t)
    {
        int a=0,j=0;
    for(int i=0;i<s.size();i++)
        for( j;j<t.size();j++)
                if(s[i]==t[j]){ a++;i++;}

    if(a==s.size())
        cout<<"Yes\n";
    else
        cout<<"No\n";
    }
    return 0;
}
