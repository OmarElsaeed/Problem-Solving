#include <iostream>
#include<math.h>

using namespace std;

int main()
{
    double n, t, k, d;
    cin>>n>>t>>k>>d;
    if(d<t&&n>k)
        cout<<"YES\n";
        else if (t<d&&(int)d%(int)t!=0&&(n-ceil(d/t)*k)>=1)
            cout<<"YES\n";
        else if ((t==d||(int)d%(int)t==0)&&(n-(d/t)*k)>k)

             cout<<"YES\n";

    else
        cout<<"NO\n";
    return 0;
}
