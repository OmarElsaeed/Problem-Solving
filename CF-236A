#include <bits/stdc++.h>

using namespace std;

int main()
{

    string w;
    cin >> w;
    sort(w.begin(), w.end());
    w.erase(unique(w.begin(), w.end()), w.end());
    if (w.length() % 2 == 0)
    {
        cout << "CHAT WITH HER!" << endl;
    }
    else
    {
        cout << "IGNORE HIM!" << endl;
    }
    return 0;
}
